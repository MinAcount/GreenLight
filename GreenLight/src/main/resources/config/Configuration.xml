<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration PUBLIC "-//mybatis.org//DTD Config 3.0//EN" "http://mybatis.org/dtd/mybatis-3-config.dtd" >
<configuration>
	<typeAliases>
		<!-- 인사 -->
		<typeAlias type="com.green.light.vo.EmployeeVo" alias="EmpVo"/>
		<typeAlias type="com.green.light.vo.DepartmentVo" alias="DeptVo"/>
		<typeAlias type="com.green.light.vo.HeadquartersVo" alias="HeadVo"/>
		<typeAlias type="com.green.light.vo.SignVo" alias="SignVo"/>
		<typeAlias type="com.green.light.vo.AttendanceVo" alias="AttendanceVo"/>
		<typeAlias type="com.green.light.vo.VacationVo" alias="VacationVo"/>
		<!-- 일정 -->
		<typeAlias type="com.green.light.vo.ScheduleVo" alias="ScheduleVo"/>
		<!-- 회의실 -->
		<typeAlias type="com.green.light.vo.ConferenceVo" alias="ConferenceVo"/>
		<typeAlias type="com.green.light.vo.ReservationVo" alias="ReservationVo"/>
		<typeAlias type="com.green.light.vo.CheckListVo" alias="CheckListVo"/>
		<!-- 전자결재 -->
		<typeAlias type="com.green.light.vo.DocumentVo" alias="DocumentVo"/>
		<typeAlias type="com.green.light.vo.ApprovalVo" alias="ApprovalVo"/>
		<typeAlias type="com.green.light.vo.TemplateVo" alias="TemplateVo"/>
		<typeAlias type="com.green.light.vo.ApprJstreeVo" alias="ApprJstreeVo"/>
		<!-- 채팅 -->
		<typeAlias type="com.green.light.vo.MessageVo" alias="MessageVo"/>
		<typeAlias type="com.green.light.vo.GroupMemberVo" alias="GroupMemberVo"/>
		<!-- 외부 라이브러리 -->
		<typeAlias type="com.green.light.vo.JsTreeVo" alias="JsTreeVo"/>
	</typeAliases>
	
	<mappers>
		<!-- 인사 -->
		<mapper resource="sqls/EmployeeStatement.xml"/>
		<mapper resource="sqls/DepartmentStatement.xml"/>
		<mapper resource="sqls/HeadquartersStatement.xml"/>
		<mapper resource="sqls/SignatureStatement.xml"/>
		<mapper resource="sqls/AttendanceStatement.xml"/>
		<mapper resource="sqls/VacationStatement.xml"/>
		<!-- 일정 -->
		<mapper resource="sqls/ScheduleStatement.xml"/>
		<!-- 회의실 -->
		<mapper resource="sqls/ConferenceStatement.xml"/>
		<mapper resource="sqls/ReserveStatement.xml"/>
		<!-- 전자결재 -->
		<mapper resource="sqls/ApprovalStatement.xml"/>
		<mapper resource="sqls/DocumentStatement.xml"/>
		<mapper resource="sqls/TemplateStatement.xml"/>
		<!-- 채팅 -->
		<mapper resource="sqls/MessageStatement.xml"/>
		<!-- 외부 라이브러리 -->
		<mapper resource="sqls/JsTreeStatement.xml"/>
	</mappers>
</configuration>
